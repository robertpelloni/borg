{
  "$schema": "https://opencode.ai/config.json",
  "username": "FAIZ",
  "autoupdate": true,
  "plugin": [
    "opencode-openai-codex-auth@4.2.0",
    "opencode-antigravity-auth@1.2.6",
    "@nick-vi/opencode-type-inject@1.3.1",
    "@franlol/opencode-md-table-formatter@0.0.3",
  ],
  "instructions": ["./instructions/*.md"],
  "tui": {
    "scroll_speed": 5,
  },
  "permission": {
    "bash": {
      "make *": "allow",
      "echo *": "allow",
      "grep *": "allow",
      "cat *": "allow",
    },
  },
  "mcp": {
    "zai-mcp-server": {
      "type": "local",
      "command": ["npx", "-y", "@z_ai/mcp-server"],
      "environment": {
        "Z_AI_API_KEY": "YOUR_Z_AI_API_KEY_HERE",
        "Z_AI_MODE": "ZAI",
      },
    },
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "YOUR_CONTEXT7_API_KEY_HERE",
      },
      "enabled": true,
    },
    "next-devtools": {
      "type": "local",
      "command": ["npx", "-y", "next-devtools-mcp@latest"],
      "enabled": true,
    },
    "Ref": {
      "type": "remote",
      "url": "https://api.ref.tools/mcp?apiKey=ref-458b34e9855ea394143a",
      "enabled": false,
    },
    "exa": {
      "type": "remote",
      "url": "https://mcp.exa.ai/mcp?tools=web_search_exa,get_code_context_exa,crawling_exa,company_research_exa,linkedin_search_exa,deep_researcher_start,deep_researcher_check",
      "enabled": false,
      "headers": {
        "EXA_API_KEY": "{env:EXA_API_KEY}",
      },
    },
    "supabase": {
      "type": "remote",
      "url": "https://mcp.supabase.com/mcp?project_ref=zxyequaeestijzerrcrx&features=docs%2Caccount%2Cdatabase%2Cdebugging%2Cdevelopment%2Cfunctions%2Cbranching%2Cstorage",
      "enabled": false,
      "headers": {
        "Authorization": "Bearer YOUR_SUPABASE_BEARER_TOKEN_HERE",
      },
    },
    "chrome-devtools": {
      "type": "local",
      "command": ["npx", "-y", "chrome-devtools-mcp@latest"],
      "enabled": false,
    },
    "kibo-ui": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "mcp-remote",
        "https://www.kibo-ui.com/api/mcp/mcp",
      ],
      "enabled": false,
    },
    "firecrawl-mcp": {
      "type": "local",
      "command": ["npx", "-y", "firecrawl-mcp"],
      "environment": {
        "FIRECRAWL_API_KEY": "YOUR_FIRECRAWL_API_KEY_HERE",
      },
      "enabled": true,
    },
    "morph-mcp": {
      "type": "local",
      "command": ["npx", "-y", "@morphllm/morphmcp"],
      "environment": {
        "MORPH_API_KEY": "YOUR_MORPH_API_KEY_HERE",
        "ENABLED_TOOLS": "edit_file,warpgrep_codebase_search",
      },
    },
  },
  "provider": {
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": ["reasoning.encrypted_content"],
        "store": false,
      },
      "models": {
        "gpt-5.2-high": {
          "name": "GPT 5.2 High (OAuth)",
          "limit": {
            "context": 272000,
            "output": 128000,
          },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"],
          },
          "options": {
            "reasoningEffort": "high",
            "reasoningSummary": "detailed",
            "textVerbosity": "medium",
            "include": ["reasoning.encrypted_content"],
            "store": false,
          },
        },
        "gpt-5.2-codex-high": {
          "name": "GPT 5.2 Codex High (OAuth)",
          "limit": {
            "context": 272000,
            "output": 128000,
          },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"],
          },
          "options": {
            "reasoningEffort": "high",
            "reasoningSummary": "detailed",
            "textVerbosity": "medium",
            "include": ["reasoning.encrypted_content"],
            "store": false,
          },
        },
        "gpt-5.2-xhigh": {
          "name": "GPT 5.2 Extra High (OAuth)",
          "limit": {
            "context": 272000,
            "output": 128000,
          },
          "modalities": {
            "input": ["text", "image"],
            "output": ["text"],
          },
          "options": {
            "reasoningEffort": "xhigh",
            "reasoningSummary": "detailed",
            "textVerbosity": "medium",
            "include": ["reasoning.encrypted_content"],
            "store": false,
          },
        },
      },
    },
    "google": {
