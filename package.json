{
  "name": "super-ai-plugin",
  "version": "0.2.5",
  "private": true,
  "scripts": {
    "start": "concurrently \"pnpm --filter @super-ai/core dev\" \"pnpm --filter @super-ai/ui dev\"",
    "dev:full": "concurrently \"pnpm --filter @super-ai/core dev\" \"pnpm --filter @super-ai/ui dev\"",
    "dev": "pnpm --filter @super-ai/core dev",
    "build": "pnpm --filter \"./packages/*\" build",
    "start:all": "pnpm run build && pnpm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "jsonpath-plus": "^10.3.0"
  }
}
