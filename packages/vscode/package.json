{
  "name": "super-ai-vscode",
  "displayName": "Super AI Plugin",
  "version": "0.1.0-skeleton",
  "publisher": "super-ai",
  "engines": {
    "vscode": "^1.85.0"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
      "onCommand:super-ai.connect"
  ],
  "contributes": {
    "commands": [
      {
        "command": "super-ai.connect",
        "title": "Super AI: Connect to Hub"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "package": "vsce package",
    "test": "npm run compile && node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "20.x",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "socket.io-client": "^4.7.4"
  }
}
